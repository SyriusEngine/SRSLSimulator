ShaderType = Vertex;

Input vsIn{
    float3 position: Position;
    float3 color: Color;
    float3 normal: Normal;
    float2 texCoord: TexCoords;
};

Output vsOut{
    float4 position: SRV_POSITION;
    float4 color: Color;
};

void main(){
    vsOut.position = float4(vsIn.position, 1.0);
    vsOut.color = float4(vsIn.color, 1.0);
}